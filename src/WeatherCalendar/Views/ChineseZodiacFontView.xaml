<rxui:ReactiveUserControl
    x:Class="WeatherCalendar.Views.ChineseZodiacFontView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rxui="http://reactiveui.net"
    xmlns:vms="clr-namespace:WeatherCalendar.ViewModels"
    x:TypeArguments="vms:ChineseZodiacFontViewModel"
    mc:Ignorable="d">
    <Grid>
        <TextBlock
            x:Name="ChineseZodiacTextBlock"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            FontFamily="{DynamicResource ChineseZodiacFontFamily}"
            FontSize="70"
            Text="&#xe663;" />
    </Grid>
</rxui:ReactiveUserControl>
